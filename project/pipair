#!/bin/bash
SCRIPTDIR=$(cd $(dirname "$0") && pwd)
CUR_PATH=$(pwd)
if [ "$#" == 1 ]; then
#printf "enter 1\n"
opt -print-callgraph $1 2>&1 | java -Xms128m -Xmx128m -cp $SCRIPTDIR/pipairJAVA/bin pipairJAVA.ControllerMain 2>/dev/null
elif [ "$#" == 3 ]; then
opt -print-callgraph $1 2>&1 | java -Xms128m -Xmx128m -cp $SCRIPTDIR/pipairJAVA/bin pipairJAVA.ControllerMain $2 $3 2>/dev/null
elif [ "$#" == 4 ]; then
opt -print-callgraph $1 2>&1 | java -Xms128m -Xmx128m -cp $SCRIPTDIR/pipairJAVA/bin pipairJAVA.ControllerMain $2 $3 $4 2>/dev/null
else
printf "please give me arguments <bitcode_file> or <bitcode_file> <T_SUPPORT> <T_CONFIDENCE>\n"
fi
